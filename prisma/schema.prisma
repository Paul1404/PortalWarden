// schema.prisma

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ValidTag {
  id        Int      @id @default(autoincrement())
  tag       String   @unique
  timestamp DateTime @default(now())
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

